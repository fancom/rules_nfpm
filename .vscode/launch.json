    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch nfpm_wrapper",
            "type": "go",
            "request": "launch",
            "mode": "exec",
            "program": "bazel-bin/go/cmd/nfpmwrapper/nfpmwrapper_/nfpmwrapper",
            "env": {},
            "cwd": "${workspaceFolder}",
            "args": [
                "--config=tests/test-config.yaml",
                "--stable-status=bazel-out/stable-status.txt",
                "--volatile-status=bazel-out/volatile-status.txt",
                "--dep='@//tests:meow.txt=tests/meow.txt'",
                "--dep='@//tests:meow2.txt=tests/meow2.txt'",
                "bazel-out/k8-fastbuild/bin/tests/test.rpm"
                //"${workspaceFolder}/cmd/system_management_service/config.json"
            ],
            "trace": "verbose",
            "substitutePath": [
                {
                    "from": "${workspaceFolder}/bazel-${workspaceFolderBasename}/external",
                    "to": "external/",
                },
                {
                    "from": "${workspaceFolder}/bazel-${workspaceFolderBasename}/external",
                    "to": "GOROOT/",
                },
                {
                    "from": "${workspaceFolder}/go",
                    "to": "go/",
                },

                {
                    "from": "/home/az/go/pkg/mod/github.com/goreleaser/nfpm/v2@v2.26.0/",
                    "to": "/home/az/Projects/fancom/bazel/rules_nfpm/bazel-rules_nfpm/external/com_github_goreleaser_nfpm_v2",
                }
                
            ],
            "logOutput": "debugger,dap",
            "showLog": true,
        }
    ]
}